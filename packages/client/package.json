{
  "name": "@better-bull-board/client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "0.0.10",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "debug": "npx -y dotenv-cli -e .env -- tsx debug/index.ts",
    "prepublishOnly": "npm run build"
  },
  "license": "ISC",
  "dependencies": {
    "@rharkor/logger": "^1.3.3",
    "@t3-oss/env-core": "^0.13.4",
    "bullmq": "^5.58.6",
    "dotenv": "^17.0.0",
    "ioredis": "^5.7.0",
    "tsdown": "^0.15.2",
    "typescript": "^5.9.2",
    "zod": "^3.25.76"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^24.5.2",
    "tsx": "^4.19.3"
  }
}
